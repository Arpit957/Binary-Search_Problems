//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
class Solution{
    public:
    int kthElement(int arr1[], int arr2[], int n, int m, int k)
    {
        int i = 0;
        int j = 0;
        int temp = 1;
        if (k > n + m) {
          return -1; // k is out of range
        }
        while(i<n && j<m){
            if(arr1[i]<=arr2[j]){
                if(temp==k) return arr1[i];
                i++;
            }
            else{
            if(temp==k) return arr2[j];
                j++;
            }
             temp++;
        }
        while (i < n) {
            if (temp == k) return arr1[i];
            i++;
            temp++;
        }
        while (j < m) {
            if (temp == k) return arr2[j];
            j++;
            temp++;
        }
    }
};

//{ Driver Code Starts.
 
// Driver code
int main()
{
	int t;
	cin>>t;
	while(t--){
		int n,m,k;
		cin>>n>>m>>k;
		int arr1[n],arr2[m];
		for(int i=0;i<n;i++)
			cin>>arr1[i];
		for(int i=0;i<m;i++)
			cin>>arr2[i];
		
		Solution ob;
        cout << ob.kthElement(arr1, arr2, n, m, k)<<endl;
	}
    return 0;
}
// } Driver Code Ends
